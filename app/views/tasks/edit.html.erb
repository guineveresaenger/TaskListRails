<%= form_for @task, method: :patch do |f| %>
<%= f.label :title %>
<%= f.text_field :title %>
<%= f.label :description %>
<%= f.text_field :description %>
<%= f.label :completed_at, "What time did you finish this task?"%>
<%= f.time_field :completed_at %>

<%= f.submit "Go make me work!" %>
<% end %>
